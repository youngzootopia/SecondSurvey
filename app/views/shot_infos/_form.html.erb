<%= simple_form_for(@shot_info) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :ShotID %>
    <%= f.input :ShotNum %>
    <%= f.input :StartFrame %>
    <%= f.input :EndFrame %>
    <%= f.input :ThumbURL %>
    <%= f.input :CID %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
