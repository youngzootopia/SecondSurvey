<p id="notice"><%= notice %></p>

<h1>First Query Tags</h1>

<table>
  <thead>
    <tr>
      <th>Queryid</th>
      <th>Shotid</th>
      <th>Tagdesc</th>
      <th>Tagid</th>
      <th>Tagscore</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @first_query_tags.each do |first_query_tag| %>
      <tr>
        <td><%= first_query_tag.queryID %></td>
        <td><%= first_query_tag.shotID %></td>
        <td><%= first_query_tag.tagDesc %></td>
        <td><%= first_query_tag.tagID %></td>
        <td><%= first_query_tag.tagScore %></td>
        <td><%= link_to 'Show', first_query_tag %></td>
        <td><%= link_to 'Edit', edit_first_query_tag_path(first_query_tag) %></td>
        <td><%= link_to 'Destroy', first_query_tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New First Query Tag', new_first_query_tag_path %>
